<p id="notice"><%= notice %></p>

<h1>Sleep Plan Histories</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Coreid</th>
      <th>Sleep count</th>
      <th>First publish</th>
      <th>Last</th>
      <th>Min plan</th>
      <th>Mean reading</th>
      <th>Total plan</th>
      <th>Max plan</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sleep_plan_histories.each do |sleep_plan_history| %>
      <tr>
        <td><%= sleep_plan_history.date %></td>
        <td><%= sleep_plan_history.coreid %></td>
        <td><%= sleep_plan_history.sleep_count %></td>
        <td><%= sleep_plan_history.first_publish %></td>
        <td><%= sleep_plan_history.last_id %></td>
        <td><%= sleep_plan_history.min_plan %></td>
        <td><%= sleep_plan_history.mean_reading %></td>
        <td><%= sleep_plan_history.total_plan %></td>
        <td><%= sleep_plan_history.max_plan %></td>
        <td><%= link_to 'Show', sleep_plan_history %></td>
        <td><%= link_to 'Edit', edit_sleep_plan_history_path(sleep_plan_history) %></td>
        <td><%= link_to 'Destroy', sleep_plan_history, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sleep Plan History', new_sleep_plan_history_path %>
